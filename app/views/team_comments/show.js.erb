teamCommentList.insertAdjacentHTML("afterbegin", `<div class="comment-header">
              <img src="<%= @team_comment.user.profile_avatar_url %>" class="avatar">
              <p><%= link_to @team_comment.user.username, user_path(@team_comment.user) %> <em><%= distance_of_time_in_words(Time.now - @team_comments.created_at) %> ago</em></p>
            </div>
            <div class="comment-content">
              <p><%= @team_comment.content %></p>
            </div>`);
